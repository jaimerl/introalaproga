#include "stdafx.h"
#include "conio.h"
#include <iostream>
using namespace std;
void main(){
	float a,b,c,d,e,f,x,y,D,Dx,Dy;
	cin>>a;
	cin>>b;
	cin>>c;
	cin>>d;
	cin>>e;
	cin>>f;
	D=a*e-b*d;
	Dx=c*e-b*f;
	Dy=a*f-c*d;
	x=Dx/D;
	y=Dy/D;
	if(D==0){
		if(Dx==0&&Dy==0){
			cout<< "Infinitas solucuones ";}
		else {
			cout<< "No hay solucion ";}
	}
	else{
	cout<< "Los resultados son: para x= "<<x<<" y para y= "<<y;
	}
	system ("pause");
}
