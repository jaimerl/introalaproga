#include "stdafx.h"
#include "conio.h"
#include <iostream>

using namespace std;
void main () {
	int n,ep,p1,p2,p3,ef,id,I,nf;
	cout<< "ingrese el numero de estudiantes: ";
	cin>> n;
		for (I=1;I<=n;I++){
			cout<< "ingrese su ID: ";
			cin>> id;
			cout<< "ingrese la nota practica: ";
			cin>> ep;
			cout<< "parcial 1: ";
			cin>> p1;
			cout<< "parcial 2: ";
			cin>> p2;
			cout<< "parcial 3: ";
			cin>> p3;
			cout<< "ingrese la nota del examen final: ";
			cin>> ef;
			nf= ep*0.2+((p1+p2+p3)/3)*0.4+ef*0.4;
			cout<< "la nota final es: "<<nf<<endl;
		}
		getch();
}

