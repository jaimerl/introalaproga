#include "stdafx.h"
#include "conio.h"
#include <iostream>

using namespace std;

void main(){
	int a,b,c;
	cin>> a;
	cin>> b;
	cin>> c;
	if(a<b&&b<c){
		cout<<a<<b<<c;}
	else{
		if (b<a&&a<c){
		cout<<b<<a<<c;}
	else{
		if(c<a&&a<b){
			cout<<c<<a<<b;}
		else{
	    if(c<b&&b<a){
			cout<<c<<a<<b;}
		else{
		if(c<b&&b<a){
			cout<<c<<b<<a;}
		else{
		if (a<c&&c<b){
			cout<<a<<c<<b;}
		else{
		    cout<<b<<c<<a;}
		}}}}}
	getch();
}

	

		

		


